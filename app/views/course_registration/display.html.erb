<h1>Listing All Students</h1>

<table class = "table table-striped">
  <thead>
  <tr>
    <th>Course Name</th>
    <th>Student Name</th>
    <th>Student-Course Status</th>
  </thead>

  <tbody>
  <tr>
    <% @student_list.each do |slist|
    %>
      <td><%= Course.find_by(id: slist.courseid).name %></td>
      <td><%= Student.find_by(id: slist.studentid).name %></td>
      <td><%= slist.status %></td>
      </tr>
    <% end %>
    </tbody>
</table>

