<p id="notice"><%= notice %></p>
<h1>Listing Courses for a particular Major</h1>

<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Major</th>
    <th></th>
  </thead>

  <tbody>
  <% @final_course.each do |course|
  @course_id = course.id%>
    <tr>
      <td><%= course.name %></td>
      <td><%= course.discipline %></td>
      <td><%=button_to 'register', view_course_path(params: {'teacherid' => @teacher_id, 'courseid' => @course_id })%></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>